# Judo Framework - Environment Variables Example
# Copy this file to .env and fill in your actual values
# The .env file should NOT be committed to version control

# API Authentication
API_TOKEN=Bearer your_jwt_token_here
API_KEY=your_api_key_here
X_API_KEY=your_x_api_key_here

# Base URLs
BASE_URL=https://api.example.com
DEV_BASE_URL=https://dev-api.example.com
STAGING_BASE_URL=https://staging-api.example.com
PROD_BASE_URL=https://api.example.com

# OAuth / Authentication
CLIENT_ID=your_client_id
CLIENT_SECRET=your_client_secret
OAUTH_TOKEN=your_oauth_token

# Custom Headers
CUSTOM_HEADER_1=value1
CUSTOM_HEADER_2=value2

# Test Configuration (for dynamic variables)
TEST_API_URL=https://jsonplaceholder.typicode.com
TEST_USER_ID=1
TEST_POST_TITLE=Mi Post de Prueba
USER_ID=12345
ENDPOINT_SUFFIX=/v2/data

# Authentication Tokens
AUTH_TOKEN=Bearer abc123
TENANT_ID=tenant_abc

# Other Configuration
TIMEOUT=30
MAX_RETRIES=3
